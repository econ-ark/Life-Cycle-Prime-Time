@article{Andrews2017,
  title     = {Measuring the sensitivity of parameter estimates to estimation
               moments},
  author    = {Andrews, Isaiah and Gentzkow, Matthew and Shapiro, Jesse M},
  abstract  = {AbstractWe propose a local measure of the relationship between
               parameter estimates and the moments of the data they depend on.
               Our measure can be computed at negligible cost even for complex
               structural models. We argue that reporting this measure can
               increase the transparency of structural estimates, making it
               easier for readers to predict the way violations of identifying
               assumptions would affect the results. When the key assumptions
               are orthogonality between error terms and excluded instruments,
               we show that our measure provides a natural extension of the
               omitted variables bias formula for nonlinear models. We
               illustrate with applications to published articles in several
               fields of economics.},
